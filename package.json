{
  "name": "playball",
  "version": "2.2.1",
  "description": "Watch MLB games from the comfort of your terminal",
  "keywords": [
    "MLB",
    "gameday",
    "baseball",
    "cli",
    "terminal"
  ],
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile",
    "clean": "rimraf ./dist",
    "compile": "babel src --out-dir dist",
    "lint": "eslint src",
    "start": "babel-node ./src/main.js",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/main.js",
  "bin": {
    "playball": "./bin/playball.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paaatrick/playball.git"
  },
  "author": "Patrick Kalita <patrick.kalita@gmail.com> (http://paaatrick.github.io/)",
  "bugs": {
    "url": "https://github.com/paaatrick/playball/issues"
  },
  "homepage": "https://github.com/paaatrick/playball",
  "preferGlobal": true,
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "blessed": "^0.1.81",
    "immutable": "^3.8.2",
    "json-patch": "^0.7.0",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-blessed": "^0.5.0",
    "react-devtools-core": "^3.6.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "winston": "^3.3.3",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "rimraf": "^2.6.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
